    "babel-plugin-styled-components": "^2.0.7",
    "cypress": "^10.0.3",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.5.12",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "4.7.3"
